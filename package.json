{
  "name": "sample-mobile-react",
  "version": "1.0.0",
  "description": "react移动端模板项目",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development UMI_ENV=dev PORT=10000 APP_TYPE=site umi dev",
    "build": "cross-env NODE_ENV=production UMI_ENV=prod  umi build",
    "test": "umi test",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint": "eslint --ext .js src && npm run lint:style",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:fix": "eslint --fix --ext .js src && npm run lint:style",
    "tslint": "npm run tslint:fix",
    "tslint:fix": "tslint --fix 'src/**/*.ts*'",
    "lint-staged:js": "eslint --ext .js",
    "prettier": "node ./scripts/prettier.js"
  },
  "dependencies": {
    "@ionic/react": "0.0.10",
    "antd-mobile": "^2.2.14",
    "better-scroll": "^1.13.2",
    "classnames": "^2.2.6",
    "dva": "^2.4.0",
    "enzyme": "^3.10.0",
    "glob": "^7.1.3",
    "hash.js": "^1.1.5",
    "ionicons": "^4.6.2",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "nprogress": "^0.2.0",
    "nzh": "^1.0.4",
    "prop-types": "^15.5.10",
    "qs": "^6.5.2",
    "react": "^16.9.0",
    "react-time-line": "^0.2.0",
    "save": "^2.4.0",
    "store": "^2.0.12"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "browser-detect": "^0.2.28",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.1",
    "cross-env": "^5.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-umi": "^1.5.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.0.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^4.0.0",
    "umi": "^2.8.17",
    "umi-plugin-authorize": "^1.0.3",
    "umi-plugin-auto-externals": "^1.2.8",
    "umi-plugin-react": "^1.9.15"
  },
  "lint-staged": {},
  "engines": {
    "node": ">=8.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
